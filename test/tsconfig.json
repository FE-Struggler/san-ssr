{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "outDir": "dist",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "strict": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "all": true,
  "exclude": [ "node_modules", "dist" ]
}
